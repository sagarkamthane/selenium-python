    wait = WebDriverWait(driver, 20)
    wait.until(expected_conditions.element_to_be_clickable((By.CSS_SELECTOR, 'button[data-role="closeBtn"]')))
    driver.find_element_by_css_selector('button[data-role="closeBtn"]')
    wait = WebDriverWait(driver, 20)
    wait.until(expected_conditions.element_to_be_clickable((By.CSS_SELECTOR, 'a[class *= "accept"]')))
    driver.find_element_by_css_selector('a[class *= "accept"]').click()